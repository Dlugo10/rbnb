<h1 class="border-bottom">Flats</h1>

<ul class='task-list'>
  
  <% @flats.each do |flat|%>
    <li>
      <% if flat.completed %>
        <i class="fa fa-check-square"></i>
      <% else %>
        <i class="fa fa-square"></i>
      <% end %>
      <%= link_to flat.price, flat_path(task) %>
      
    </li>
  <% end %>
</ul>
